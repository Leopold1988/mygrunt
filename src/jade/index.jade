extends model/main

block page-body
  script.
    window.domy = {};
    domy.toggle = {
      a : true
    };
  .w1000
    ul#metroUl
      - for (var x = 0; x < 12; x++)
          li
            .drop.cell
              p.middle #{describe}

    #center
      .ceil
        ul.center
          - for (var x = 0; x < 4; x++)
              li
    #countDown 100

    #popup
      input#inline(type="button", value="inline弹出层", data-which="pop1")
      br
      input#ajax(type="button", value="ajax弹出层", data-url="../model/popup_model/confirm.html")
      #pop1.hipopupbox(hipopup="inline")
        a.close
        .inner
          | 1234
          br
          | 1234
          br
          | 1234
          br
    #format
      .unix
      .date
    #placeholder
      input(type="text",placeholder="请输入")
      .suggest
        ul
          li aaaaaaaa
    #strength
      input(type="text",placeholder="密码强度")
      span
    #hover
      .style1
        a(href="javascript:;") mouseover显示隐藏层
        .hide 可以移入
      .style2
        a(href="javascript:;") mouseover显示隐藏层
        .hide 不可以移入
    #balloonBubbles
      .bbbtn
        .aaa help
    #btn
      a.button_blue(href="javascript:;") .button_blue
      a.button_gray(href="javascript:;") .button_gray
      a.button_style2(href="javascript:;") .button_style2
      a.button_style3(href="javascript:;") .button_style3
      a.button_style4(href="javascript:;") .button_style4
      a.button_style5(href="javascript:;") .button_style5
    #metrotest
      ul
